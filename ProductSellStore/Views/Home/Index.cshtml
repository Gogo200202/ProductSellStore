@{
    ViewData["Title"] = "Home Page";
}


<div id="carouselExampleControlsNoTouching" class="carousel slide" data-bs-touch="false">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://as1.ftcdn.net/v2/jpg/02/41/43/18/1000_F_241431868_8DFQpCcmpEPVG0UvopdztOAd4a6Rqsoo.jpg" class="rounded mx-auto d-block" alt="Photo 1">
        </div>
        <div class="carousel-item">
            <img src="https://as2.ftcdn.net/v2/jpg/02/74/98/91/1000_F_274989153_XSiWoHsVOnlTs0Far2EYacdg8rE4kw27.jpg" class="rounded mx-auto d-block" alt="Photo 2">
        </div>
        <div class="carousel-item">
            <img src="https://as1.ftcdn.net/v2/jpg/03/93/88/78/1000_F_393887833_JuP5DkgghppS6vSMA55vBzSH7m5NBbkL.jpg" class="rounded mx-auto d-block" alt="Photo 3">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControlsNoTouching" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>

<h1>Products</h1>
@await Html.PartialAsync("_RenderItemsPartial");