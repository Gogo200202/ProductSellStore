@model List<ProductSellStore.ViewModel.AllOrederesViweModel>
<div class="container mt-5">
    <div class="d-flex justify-content-center row">
        <div class="col-md-10">
            <div class="rounded">
                <div class="table-responsive table-borderless">
                    <table class="table">
                        <thead>
                        <tr>
                            <th class="text-center">
                                <div class="toggle-btn">
                                    <div class="inner-circle"></div>
                                </div>
                            </th>
                            <th>Order id</th>
                            <th>User name</th>
                            <th>First name</th>
                            <th>Last name</th>
                            <th>Email</th>
                                <th>Address</th>
                                <th>Country</th>
                                <th>State</th>
                            <th>Zip</th>
                                <th>Description</th>
                            <th>status</th>
                            <th>Total</th>
                            <th>Created</th>
                            <th></th>
                        </tr>
                        </thead>
                    @{
                        foreach (var curentModel in Model)
                        {
                            <tbody class="table-body">
                            <tr class="cell-1">
                                <td class="text-center">
                                    <div class="toggle-btn">
                                        <div class="inner-circle"></div>
                                    </div>
                                </td>
                               
                                <td>@curentModel.Id</td>
                                <td>@curentModel.User.UserName</td>
                                <td>@curentModel.FirstName</td>
                                <td>@curentModel.LastName</td>
                                <td>@curentModel.Email</td>
                                        <td>@curentModel.Address</td>
                                        <td>@curentModel.Country</td>
                                        <td>@curentModel.State</td>
                                        <td>@curentModel.Zip</td>
                                        <td>@curentModel.Description</td>
                                <td>@curentModel.AllStatus</td>
                                <td>@curentModel.Item.Price</td>
                                        <td>@curentModel.CreatedOn</td>
                                
                                
                            </tr>
                          
                            </tbody>
                        }
                    }

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>